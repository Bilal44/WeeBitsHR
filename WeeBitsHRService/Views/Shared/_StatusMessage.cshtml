@if (TempData["SM"] != null)
{
    <div class="text-start alert alert-success alert-dismissible" role="alert">
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        @Html.Raw(TempData["SM"])
    </div>
}

@if (TempData["Error"] != null)
{
    <div class="text-start alert alert-danger alert-dismissible" role="alert">
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        @Html.Raw(TempData["Error"])
    </div>
}

@if (TempData["Warning"] != null)
{
    <div class="text-start alert alert-warning alert-dismissible" role="alert">
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        @Html.Raw(TempData["Warning"])
    </div>
}